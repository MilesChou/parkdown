{
    "name": "mileschou/parkdown",
    "description": "The markdown parser for PHP",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "markdown"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/container": "^6 | ^7",
        "illuminate/filesystem": "^6 | ^7",
        "illuminate/pipeline": "^6 | ^7"
    },
    "require-dev": {
        "erusev/parsedown": "^1.7",
        "phpstan/phpstan": "^0.12.11",
        "phpunit/phpunit": "^8 | ^9",
        "psy/psysh": "^0.9.12",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/yaml": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "MilesChou\\Parkdown\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
